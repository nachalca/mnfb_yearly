journal = {Journal of Statistical Planning and Inference},
keywords = {dirichlet process prior,duncan,hierarchical priors,quasi-bayesian,s procedure},
number = {1-2},
pages = {215--227},
title = {{Bayesian perspectives on multiple comparisons}},
volume = {82},
