journal = {The Plant Cell},
keywords = {Alleles,Arabidopsis,Arabidopsis: chemistry,Arabidopsis: genetics,Chromosomes,Conserved Sequence,DNA Methylation,Endosperm,Endosperm: chemistry,Endosperm: genetics,Gene Expression Profiling,Gene Expression Profiling: methods,Gene Expression Regulation,Genes,Genetic,Genetic Loci,Genomic Imprinting,Inheritance Patterns,Oryza sativa,Oryza sativa: chemistry,Oryza sativa: genetics,Plant,Plant: chemistry,Plant: genetics,Pollination,Polymorphism,RNA,Sequence Analysis,Zea mays,Zea mays: chemistry,Zea mays: genetics},
