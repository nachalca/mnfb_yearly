keywords = {allele-specific expression,epigenetic modification},
number = {4},
pages = {247--52},
pmid = {23891706},
publisher = {Beijing Institute of Genomics, Chinese Academy of Sciences and Genetics Society of China},
title = {{A computational workflow to identify allele-specific expression and epigenetic modification in maize.}},
